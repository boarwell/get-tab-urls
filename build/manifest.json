{
  "name": "get-tab-urls",
  "description": "get all tabs url",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["tabs", "activeTab", "scripting", "clipboardWrite"],
  "action": {
    "default_popup": "/popup/index.html",
    "default_icon": {
      "256": "bitmap.png"
    }
  },
  "icons": {
    "256": "bitmap.png"
  }
}
